<?xml version="1.0" encoding="utf-8"?>
<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
	<Object ObjectType="MODefinition">
		<Name>Service Endpoint</Name>
		<Description1><![CDATA[This object describes Service Endpoint that can be used by a User Equipment.]]></Description1>
		<ObjectID>36051</ObjectID>
		<ObjectURN>urn:oma:lwm2m:x:36051</ObjectURN>
		<LWM2MVersion>1.1</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="0">
				<Name>Service Name</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Name or category for this  Service Endpoint.]]></Description>
			</Item>
			<Item ID="1">
				<Name>Payload</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The type of payload that is expected by the Service Endpoint.For example: json, asn1, otlp/json, logstash, jaeger, grafana, etc.]]></Description>
			</Item>
			<Item ID="2">
				<Name>Service URI</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[URI of the Service Endpoint.]]></Description>
			</Item>
			<Item ID="3">
				<Name>Topic Root</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Root topic to be used when connecting to an MQTT Broker Endpoint.]]></Description>
			</Item>
			<Item ID="4">
				<Name>Server Public Key</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Opaque</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Stores the server's certificate, which is used by the client to validate the server's identity.]]></Description>
			</Item>

		</Resources>
		<Description2><![CDATA[]]></Description2>
	</Object>
</LWM2M>
